{
  "presets": [
    ["@babel/preset-env", {
      "modules": "commonjs",
      "targets": {
        "node": true
      }
    }],
    ["@babel/preset-typescript"]
  ],
  "plugins": [
    ["@babel/plugin-proposal-object-rest-spread", {"useBuiltIns": true}],
    ["@babel/plugin-proposal-optional-chaining"]
  ]
}
